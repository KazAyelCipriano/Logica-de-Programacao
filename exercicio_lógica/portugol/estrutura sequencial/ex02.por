programa {
  funcao inicio() {
    inteiro num1,num2,num3,resul

    escreva("Digite o 1° número: ")
    leia(num1)
    escreva("Digite o 2° número: ")
    leia(num2)
    escreva("Digite o 3° número: ")
    leia(num3)

    resul = num1*num2*num3

    escreva(num1," x ",num2," x ",num3," = ",resul)
  }
}
